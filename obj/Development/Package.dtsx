LEFT JOIN DW_KENTFOODS.dbo.DimCiudadEmpleado AS CE
    ON CE.nombre_ciudad = E.Ciudad
LEFT JOIN DW_KENTFOODS.dbo.DimPaisEmpleado AS PE
