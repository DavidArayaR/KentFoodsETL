                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH FE_UNICAS AS (
    SELECT DISTINCT 
        CAST(SUBSTRING(fecha_envio_key, 5, 8) AS VARCHAR(8)) AS fecha_yyyymmdd,
        MIN(fecha_envio_key) AS fecha_envio_key
    FROM DW_KENTFOODS.dbo.DimFechaEnvio
    GROUP BY CAST(SUBSTRING(fecha_envio_key, 5, 8) AS VARCHAR(8))
),
FO_UNICAS AS (
    SELECT DISTINCT 
        CAST(SUBSTRING(fecha_orden_key, 5, 8) AS VARCHAR(8)) AS fecha_yyyymmdd,
        MIN(fecha_orden_key) AS fecha_orden_key
    FROM DW_KENTFOODS.dbo.DimFechaOrden
    GROUP BY CAST(SUBSTRING(fecha_orden_key, 5, 8) AS VARCHAR(8))
)
SELECT
    CONCAT(
        RIGHT('000' + CAST(ROW_NUMBER() OVER (PARTITION BY O.OrdenID ORDER BY DO.ProductoID) AS VARCHAR(3)), 3),
        '_',
        CAST(O.OrdenID AS VARCHAR(10))
    ) AS fact_key,

    O.OrdenID AS orden_key,
    DO.ProductoID AS producto_key,
    O.ClienteID AS cliente_key,
    O.EmpleadoID AS empleado_key,
    O.EnviadoPor AS transportista_key,

    FE.fecha_envio_key,
    FO.fecha_orden_key,
    CE.ciudadEnvio_key,
    PE.paisEnvio_key,

    DO.PrecioUnitario,
    DO.Cantidad,
    DO.Descuento,
    (DO.PrecioUnitario * DO.Cantidad) - (DO.PrecioUnitario * DO.Cantidad * DO.Descuento) AS total

FROM KentFoods.dbo.DetalleOrden AS DO
INNER JOIN KentFoods.dbo.Ordenes AS O
    ON DO.OrdenID = O.OrdenID
LEFT JOIN FE_UNICAS AS FE
    ON FE.fecha_yyyymmdd = FORMAT(O.FechaEnvio, 'yyyyMMdd')
LEFT JOIN FO_UNICAS AS FO
    ON FO.fecha_yyyymmdd = FORMAT(O.FechaOrden, 'yyyyMMdd')
LEFT JOIN DW_KENTFOODS.dbo.DimCiudadEnvio AS CE
    ON CE.nombre_ciudad = O.CiudadEnvio
LEFT JOIN DW_KENTFOODS.dbo.DimPaisEnvio AS PE
    ON PE.nombre_pais = O.PaisEnvio
ORDER BY O.OrdenID, fact_key;</property>
