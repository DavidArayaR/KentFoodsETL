                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    E.EmpleadoID,
    E.Nombre AS nombre_empleado,
    E.Apellido AS apellido_empleado,
    E.Cargo,
    E.FechaNacimiento AS fecha_nacimiento,
